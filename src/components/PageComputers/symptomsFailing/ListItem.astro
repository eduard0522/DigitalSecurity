---
import { Icon } from 'astro-icon/components'

interface Props {
  text : string
}

const { text } = Astro.props
---

<li class=" bg-white-700 rounded-xl px-4 py-1 flex gap-2 items-center  justify-start">
 <Icon name="warning" class="text-4xl" /> <span class="text-slate-900 text-start"> { text } </span> 
</li>