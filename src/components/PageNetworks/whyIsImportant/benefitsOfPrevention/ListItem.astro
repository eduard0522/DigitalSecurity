---
import { Icon } from 'astro-icon/components'

interface Props {
  text : string
}

const { text } = Astro.props
---


<li class="bg-white-700 min-h-12 md:min-h-8 rounded-xl px-4  flex gap-2 items-center  justify-start">
  <Icon name="circle-ok" class=" min-w-8 min-h-8" /> <span class="text-slate-900 text-start"> { text } </span> 
</li>